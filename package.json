{
  "name": "nextts14-prisma5-api-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:standalone": "next build && cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public && node .next/standalone/server.js",
    "lint": "next lint",
    "scaffold": "scaffdog generate",
    "openapi:generate": "ts-node -r tsconfig-paths/register src/schemas/GenerateOpenAPI.ts",
    "license:generate": "ts-node tools/LicenseChecker.ts",
    "test:dredd": "./tests/dredd.sh"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.1",
    "@prisma/client": "^5.7.1",
    "jose": "^5.2.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "swagger-ui-react": "^5.10.5",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.6"
  },
  "devDependencies": {
    "@types/license-checker": "^25.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.3",
    "dredd": "^14.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "license-checker": "^25.0.1",
    "prisma": "^5.7.1",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-erd-generator": "^1.11.2",
    "scaffdog": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.19"
  }
}
